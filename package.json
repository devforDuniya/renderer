    "@types/node": "^20.0.0",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.1",
    "tailwindcss": "^3.3.0",
